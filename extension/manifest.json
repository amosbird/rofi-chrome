{
  // id: aocepclkpgckjeikiphffdlileoaceec
  "name": "rofi-chrome",
  "key": "",
  "version": "1.0",
  "description": "rofi with chrome",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "nativeMessaging",
    "tabs",
    "history",
    "scripting",
    "bookmarks",
    "storage",
    "sessions",
    "downloads",
    "topSites",
    "downloads.shelf",
    "clipboardRead",
    "clipboardWrite",
    "webNavigation"
  ],
  "background": {
    "service_worker": "bg.js"
  },
  "commands": {
    "switchTab": {
      "description": "switch to another tab"
    },
    "listDownloads": {
      "description": "list completed downloads"
    },
    "openHistory": {
      "description": "open tab from history"
    },
    "pageFunc": {
      "description": "page-dependent search"
    },
    "goLastTab": {
      "description": "go to last tab"
    }
  },
  "content_scripts": [
    {
      "matches": ["file:///home/amos/git/rofi-chrome/extension/tab.html"],
      "js": ["tab.js"],
      "run_at": "document_start"
    },
    {
      "matches": ["file:///home/amos/git/rofi-chrome/extension/download.html"],
      "js": ["download.js"],
      "run_at": "document_start"
    }
  ]
}
